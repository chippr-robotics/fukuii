data/
*.db
*.idb

# Ignore any local test scripts or notes
# Note: Official test scripts in test-scripts/ directory are NOT ignored
test-*.sh
!test-scripts/test-*.sh
notes.txt
TODO.txt

# Temporary files
*.tmp
*.temp
tmp/

# Backup files
*.bak
*.backup

# OS-specific files
.DS_Store
Thumbs.db

# Local testing configurations
*-local.yml
*-local.conf

